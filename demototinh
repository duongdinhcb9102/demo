<script type="text/javascript">
       async function jawab(){
           var { value: jawaban } = await swals.fire({
               title: 'vậy là cậu đồng ý tối nay 8h đi trà sữa với tớ nhóa', input: 'text', allowOutsideClick: false, showCancelButton: false,
           });
           if(jawaban && jawaban.length < 19){
           	window.jawaban = jawaban;
               pesanwhatsapp = jawaban;
               katakhir = "vậy là cậu đồng ý tối nay 8h đi trà sữa với tớ nhóa: " + jawaban;
               fullakhir();
           } else {
               await swals.fire('úi!', 'cậu không được nhập quá 18 ký tự nhóa ,thử lại đi nè');
           }
       }

